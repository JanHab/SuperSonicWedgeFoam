FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

/* 
Mach number = 2
M = u / c
c: speed of sound
c = 346.13 m/s for air at 25Â°C
u = 2 * 346.13 = 692.26 m/s
*/ 

// u   664;
u   692.26;
// u 1200;

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform ($u 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform ($u 0 0);
    }

    sym
    {
        type            symmetry;
        // type            slip;
    }

    wedge
    {
        type            slip; // as viscous wall
        // type            noSlip;
    }

    top
    {
        type            slip;
        // type            zeroGradient;
        // type            freestream;
        // freestreamValue uniform ($u 0 0);
        // type            slip;
    }

    back
    {
        type            empty;
    }

    front
    {
        type            empty;
    }

    outlet
    {
        // type           zeroGradient;    
        type            inletOutlet;
        inletValue      uniform ($u 0 0);
        value           uniform ($u 0 0);
    }
}


