FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}

solvers
{
    "p"
    {
        solver          PBiCGStab;
        preconditioner
        {
            type      GAMG;
            smoother  GaussSeidel;
        }
        tolerance       1e-06;
        relTol          1e-4;//0.01;
        minIter         2;
    }
    
    // "(U|e)"
    "(U|e|rhoU|rhoE)"
    {
        solver          PDG;//smoothSolver;
        smoother        GaussSeidel;
        tolerance       1e-06;
        relTol          1e-4;//0.01;
    }

    
    // "rho|rhoU|rhoE"
    "rho"
    {
        solver          PCG;
        preconditioner
        {
            type      AMG;
        }
        tolerance       1e-4;
        relTol          1e-4;
    }
}

