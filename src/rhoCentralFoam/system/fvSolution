{
    momentumPredictor       yes;
    consistent              yes;
    nOuterCorrectors        2;
    nCorrectors             3;
    nNonOrthogonalCorrectors 2;
    transonic               yes;
    maxCo                   0.5;
    rDeltaTSmoothingCoeff   0.1;
    rDeltaTDampingCoeff     0.5;
